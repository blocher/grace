    <div class="row">

      {#-- Hearings Column --#}
      <div class="col-md-7">

        {% for event in events %}
        <a href="{{ event.link }}">
        <div class="row hearing {{ event.class }}">

          <div class="col-xs-4 event-date center">
            <span class="event-month">{{ event.start.format('F') }}</span>
            <span class="event-day">{{ event.start.format('j') }}</span>
          </div>

          <div class="col-xs-8 event-details">
            <div class="row event-time-type">
              <span class="event-time"><i class="fa fa-clock-o"></i> {{ event.start.format('g:i A') }}</span> &mdash; {{ event.category }}
            </div>

            <div class="row">

              <h3 class="event-title">{{ event.title }}</h3>

            </div>

          </div>

        </div>
        </a>
        {% endfor %}

      </div>

      {#-- Calendar Column --#}
      <div class="col-md-5">

        {% include('partials/calendar.twig') %}

      </div>

    </div>

    <div class="see-more-wrapper row center">
      <a class="btn btn-default" href="/event">View All</a>
    </div>


</section>



					<!-- RECENT POSTS -->
					<h2>Latest News</h2>
					<div class="row recent_posts" data-appear-top-offset="-200" data-animated="fadeInUp">

						{% for post in news %}
							<div class="col-lg-4 col-md-4 col-sm-4 post_item_block">
								<div class="post_item inner">
									<div class="post_item_img">
										{% if post.thumbnail %}
											<img src="{{ post.thumbnail.src('homepage_module') }}" alt="" />
										{% endif %}
										<a class="link" href="{{ post.link }}" ></a>
									</div>
									<div class="post_item_content">
										<ul class="post_item_inf">
											<li><em>Posted on {{ post.post_date|date('F j, Y') }}</em></li>
										</ul>
										<a class="title" href="{{ post.link }}" >{{ post.title }}</a>
										<ul class="post_item_inf">
											<li>{{ post.excerpt }}</li>
										</ul>
									</div>
								</div>
							</div>
						{% endfor %}

						    <div class="see-more-wrapper row center">
						      <a class="btn btn-default" href="/news">View All News</a>
						    </div>

					</div>







					</div><!-- RECENT POSTS -->
				</div><!-- //CONTAINER -->
			</section><!-- //NEWS -->
		</div><!-- //PAGE -->



{% endblock %}